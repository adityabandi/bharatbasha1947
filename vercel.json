{
  "version": 2,
  "name": "bharatbasha-telugu-dictionary",
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/word/(.*)",
      "dest": "/word/$1"
    },
    {
      "src": "/english/(.*)",
      "dest": "/english/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}